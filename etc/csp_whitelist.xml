<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="inpost-pl" type="host">*.inpost.pl</value>
                <value id="inpost-it" type="host">*.inpost.it</value>
                <value id="geowidget" type="host">*.easypack24.net</value>
                <value id="gmaps" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="inpost-pl" type="host">geowidget.easypack24.net</value>
            </values>
        </policy>

        <policy id="font-src">
            <values>
                <value id="inpost-pl" type="host">*.easypack24.net</value>
            </values>
        </policy>

        <policy id="connect-src">
            <values>
                <value id="inpost-pl" type="host">*.inpost.pl</value>
                <value id="inpost-it" type="host">*.easypack24.net</value>
            </values>
        </policy>

        <policy id="img-src">
            <values>
                <value id="inpost" type="host">*.easypack24.net</value>
                <value id="osm" type="host">*.inpost.pl</value>
                <value id="osm-com" type="host">*.inpost.com</value>
                <value id="openstreetmap" type="host">*.openstreetmap.org</value>

            </values>
        </policy>






    </policies>
</csp_whitelist>